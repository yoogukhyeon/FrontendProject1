`\r\n                <h1>${newsContent.title}</h1>\r\n                <a href=\"/\">\r\n                    목록으로\r\n                </a>\r\n                `\r\n}\r\n\r\nfunction router(){\r\n    const routerPath = location.hash;\r\n\r\n    if(routerPath === \"\"){\r\n        newsFeeds();\r\n    }else{\r\n        newsDetail();\r\n    }\r\n}\r\n\r\nwindow.addEventListener('hashchange', router)\r\nrouter();"},"lineCount":null}},"error":null,"hash":"659cf2afa79092ee802c73eb3d3e0b7e","cacheData":{"env":{}}}